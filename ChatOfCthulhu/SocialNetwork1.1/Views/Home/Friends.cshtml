@model IEnumerable<SocialNetwork1._1.Models.ApplicationUser>

@{
    Layout = "~/Views/Shared/_LayoutHome.cshtml";
    ViewBag.Title = "UserFriends";
}

<div class="row">
    <div class="col-md-9">
        @if (Model != null)
        {
            if (Model.Count() == 0)
            {
                <h4>
                    Yo no tengo amigos :(
                    <img src="~/images/maxresdefault.jpg">
                </h4>
            }
            else
            {
                @Html.Partial("~/Views/Partial/UserList.cshtml")
            }
        }
    </div>
</div>

